{
	"name": "crm-client",
	"private": true,
	"version": "0.0.0",
	"type": "module",
	"scripts": {
		"dev": "vite",
		"build": "tsc -b && vite build",
		"preview": "vite preview",
		"prettier": "npx prettier src/**/*.{js,jsx,ts,tsx} --check",
		"prettier:write": "npx prettier src/**/*.{js,jsx,ts,tsx} --write",
		"lint": "npx eslint src/**/*.{js,jsx,ts,tsx}",
		"lint:fix": "npx eslint src/**/*.{js,jsx,ts,tsx} --fix",
		"stylelint": "npx stylelint src/**/*.{css,scss}",
		"stylelint:fix": "npx stylelint src/**/*.{css,scss} --fix",
		"lint-staged": "npx lint-staged --concurrent false",
		"prepare": "husky install"
	},
	"dependencies": {
		"react": "^18.3.1",
		"react-dom": "^18.3.1"
	},
	"devDependencies": {
		"@commitlint/cli": "^19.3.0",
		"@commitlint/config-conventional": "^19.2.2",
		"@eslint/js": "^9.6.0",
		"@types/react": "^18.3.3",
		"@types/react-dom": "^18.3.0",
		"@typescript-eslint/eslint-plugin": "^7.15.0",
		"@typescript-eslint/parser": "^7.15.0",
		"@vitejs/plugin-react": "^4.3.1",
		"eslint": "^9.6.0",
		"eslint-config-airbnb": "^19.0.4",
		"eslint-config-next": "^14.2.4",
		"eslint-config-prettier": "^9.1.0",
		"eslint-plugin-prettier": "^5.1.3",
		"eslint-plugin-react": "^7.34.3",
		"eslint-plugin-react-hooks": "^4.6.2",
		"eslint-plugin-react-refresh": "^0.4.7",
		"globals": "^15.8.0",
		"husky": "^9.0.11",
		"lint-staged": "^15.2.7",
		"prettier": "^3.3.2",
		"stylelint": "^16.6.1",
		"stylelint-config-standard": "^36.0.1",
		"stylelint-config-standard-scss": "^13.1.0",
		"typescript": "^5.5.3",
		"typescript-eslint": "^7.15.0",
		"vite": "^5.3.1"
	},
	"config": {
		"commitizen": {
			"path": "cz-conventional-changelog"
		}
	},
	"overrides": {
		"eslint": "9.3.0"
	},
	"description": "This template provides a minimal setup to get React working in Vite with HMR and some ESLint rules.",
	"main": "index.js",
	"author": "",
	"license": "ISC"
}
